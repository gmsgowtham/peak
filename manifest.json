{
  "manifest_version": 2,
  "name": "Peak",
  "version": "0.1",
  "description": "Smart sharing.",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": ["lib/javascripts/jquery.min.js", "src/javascripts/background.js"],
    "css": ["src/stylesheets/style.css"]
  }],
  "permissions": ["tabs", "<all_urls>"],
  "web_accessible_resources": ["assets/icons/*.png"]
}
